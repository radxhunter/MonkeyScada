{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://localhost:5030",
        "Protocols": "Http1AndHttp2"
      },
      "gRPC": {
        "Url": "http://localhost:5031",
        "Protocols": "Http2"
      }
    }
  },
  "SerialCommunication": {
    "PortName": "COM3",
    "BaudRate": 9600,
    "IntervalOfPollingInSeconds": 1
  },
  "redis": {
    "connectionString": "localhost"
  }
}
